{
  "compilerOptions": {
    "module": "es6",
    "baseUrl": "src",
    "jsx": "react",
    "paths": {
      "./*": ["./*"],
      "assets/*": ["assets/*"],
      "components/*": ["components/*"],
      "layout/*": ["layout/*"],
      "pages/*": ["pages/*"],
      "redux/*": ["redux/*"],
      "services/*": ["services/*"],
      "history/*": ["history/*"],
      "store/*": ["store/*"]
    }
  },
  "exclude": ["node_modules", "**/node_modules/*"],
  "include": ["src"]
}
